    <script>
        let daysItem = document.querySelector("#days");
        let hoursItem = document.querySelector("#hours");
        // let minItem = document.querySelector("#min");
        // let secItem = document.querySelector("#sec");
        var date = document.getElementsByClassName('countdown-container')[0].dataset.date;

        let countDown = () => {
            let futureDate = new Date(Date.parse(date));
             console.log(date.month);
            let currentDate = new Date();
              console.log(currentDate);
            let myDate = futureDate - currentDate;
            console.log(myDate);

            let days = Math.floor(myDate / 1000 / 60 / 60 / 24);
              console.log(days);
              

            let hours = Math.floor(myDate / 1000 / 60 / 60 ) % 24;
              console.log(hours);
            // let min = Math.floor(myDate / 1000 / 60) % 60;

            // let sec = Math.floor(myDate / 1000) % 60;
            if(days<=0 || hours<=0)
            {
                daysItem.innerHTML = "<h4> Finished </h4>";
                hoursItem.innerHTML = null;
            }

            else
            {
                daysItem.innerHTML = `${days} Days`;
                hoursItem.innerHTML = `${hours} Hours Left`;
            }
            
            // minItem.innerHTML = min;
            // secItem.innerHTML = sec;
        }

